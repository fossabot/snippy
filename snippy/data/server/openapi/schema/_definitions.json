{
  "definitions": {
    "ResponseData": {
      "required": [
        "type", 
        "attributes"
      ], 
      "type": "object", 
      "properties": {
        "attributes": {
          "$ref": "#/definitions/Attributes"
        }, 
        "type": {
          "enum": [
            "snippet", 
            "solution"
          ], 
          "type": "string"
        }, 
        "id": {
          "type": "string"
        }
      }
    }, 
    "Errors": {
      "required": [
        "meta", 
        "errors"
      ], 
      "type": "object", 
      "properties": {
        "meta": {
          "$ref": "#/definitions/Meta"
        }, 
        "errors": {
          "items": {
            "$ref": "#/definitions/Error"
          }, 
          "type": "array"
        }
      }
    }, 
    "Links": {
      "type": "object", 
      "properties": {
        "self": {
          "type": "string", 
          "description": "Link pointing to resource."
        }
      }
    }, 
    "RequestCollection": {
      "required": [
        "data"
      ], 
      "type": "object", 
      "properties": {
        "data": {
          "items": {
            "$ref": "#/definitions/RequestData"
          }, 
          "type": "array"
        }
      }
    }, 
    "ResponseResource": {
      "required": [
        "data"
      ], 
      "type": "object", 
      "properties": {
        "data": {
          "$ref": "#/definitions/ResponseData"
        }, 
        "links": {
          "$ref": "#/definitions/Links"
        }
      }
    }, 
    "ResponseMeta": {
      "required": [
        "count", 
        "limit", 
        "offset", 
        "total"
      ], 
      "type": "object", 
      "properties": {
        "count": {
          "type": "string", 
          "description": "Number of content items in data list."
        }, 
        "total": {
          "type": "string", 
          "description": "Total number of search results without limit being applied."
        }, 
        "limit": {
          "type": "string", 
          "description": "The search limit used in request."
        }, 
        "offset": {
          "type": "string", 
          "description": "The search offset used in request."
        }
      }
    }, 
    "ResponseCollectionPost": {
      "required": [
        "data"
      ], 
      "type": "object", 
      "properties": {
        "data": {
          "items": {
            "$ref": "#/definitions/ResponseData"
          }, 
          "type": "array"
        }
      }
    }, 
    "ResponseLinks": {
      "required": [
        "self", 
        "first", 
        "last"
      ], 
      "type": "object", 
      "properties": {
        "self": {
          "type": "string", 
          "description": "Pagination link to this page.", 
          "format": "uri"
        }, 
        "prev": {
          "type": "string", 
          "description": "Pagination link to previous page.", 
          "format": "uri"
        }, 
        "last": {
          "type": "string", 
          "description": "Pagination link to last page.", 
          "format": "uri"
        }, 
        "first": {
          "type": "string", 
          "description": "Pagination link to first page.", 
          "format": "uri"
        }, 
        "next": {
          "type": "string", 
          "description": "Pagination link to next page.", 
          "format": "uri"
        }
      }
    }, 
    "RequestData": {
      "required": [
        "type", 
        "attributes"
      ], 
      "type": "object", 
      "properties": {
        "attributes": {
          "$ref": "#/definitions/Attributes"
        }, 
        "type": {
          "enum": [
            "snippet", 
            "solution"
          ], 
          "type": "string"
        }
      }
    }, 
    "Meta": {
      "required": [
        "version", 
        "homepage"
      ], 
      "type": "object", 
      "properties": {
        "version": {
          "type": "string", 
          "description": "Server version."
        }, 
        "homepage": {
          "type": "string", 
          "description": "Server homepage URL."
        }
      }
    }, 
    "RequestResource": {
      "required": [
        "data"
      ], 
      "type": "object", 
      "properties": {
        "data": {
          "$ref": "#/definitions/RequestData"
        }, 
        "links": {
          "$ref": "#/definitions/Links"
        }
      }
    }, 
    "ResponseCollectionGet": {
      "required": [
        "meta", 
        "data"
      ], 
      "type": "object", 
      "properties": {
        "meta": {
          "$ref": "#/definitions/ResponseMeta"
        }, 
        "data": {
          "items": {
            "$ref": "#/definitions/ResponseData"
          }, 
          "type": "array"
        }, 
        "links": {
          "$ref": "#/definitions/ResponseLinks"
        }
      }
    }, 
    "Error": {
      "required": [
        "status", 
        "statusString", 
        "module", 
        "title"
      ], 
      "type": "object", 
      "properties": {
        "status": {
          "type": "string", 
          "description": "HTTP status code."
        }, 
        "statusString": {
          "type": "string", 
          "description": "HTTP status as text string."
        }, 
        "module": {
          "type": "string", 
          "description": "Location of error."
        }, 
        "title": {
          "type": "string", 
          "description": "Message describing the error."
        }
      }
    }, 
    "Attributes": {
      "required": [
        "data"
      ], 
      "type": "object", 
      "properties": {
        "category": {
          "enum": [
            "snippet", 
            "solution"
          ], 
          "type": "string", 
          "description": "Automatically assigned content category."
        }, 
        "updated": {
          "type": "string", 
          "description": "Optional UTC timestamp for the last update time in ISO 8601 format that is automatically assigned."
        }, 
        "group": {
          "default": "default", 
          "type": "string", 
          "description": "Optional group to which the content belongs."
        }, 
        "name": {
          "type": "string", 
          "description": "Optional alias to run the content."
        }, 
        "links": {
          "items": {
            "type": "string"
          }, 
          "type": "array", 
          "description": "Optional list of refrence links."
        }, 
        "tags": {
          "items": {
            "type": "string"
          }, 
          "type": "array", 
          "description": "Optional list of tags."
        }, 
        "versions": {
          "type": "string", 
          "description": "Optional list of services and versions related to the snippet."
        }, 
        "brief": {
          "type": "string", 
          "description": "Optional brief description of the content."
        }, 
        "filename": {
          "type": "string", 
          "description": "Optional filename that that defines automatically used filename when content is exported."
        }, 
        "created": {
          "type": "string", 
          "description": "Optional UTC timestamp for creation time in ISO 8601 format that is automatically assigned."
        }, 
        "data": {
          "items": {
            "type": "string"
          }, 
          "type": "array", 
          "description": "Mandatory content data in list context with one line per element."
        }, 
        "digest": {
          "type": "string", 
          "description": "Automatically assigned message digest that uniquely identifies the content."
        }
      }
    }
  }
}