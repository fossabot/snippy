{
  "$schema": "http://json-schema.org/schema#", 
  "required": [
    "status", 
    "statusString", 
    "module", 
    "title"
  ], 
  "type": "object", 
  "properties": {
    "status": {
      "type": "string", 
      "description": "HTTP status code"
    }, 
    "statusString": {
      "type": "string", 
      "description": "HTTP status as text string"
    }, 
    "module": {
      "type": "string", 
      "description": "Location of error"
    }, 
    "title": {
      "type": "string", 
      "description": "Message describing the error."
    }
  }
}